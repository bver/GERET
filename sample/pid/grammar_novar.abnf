
program = [ " v1 = " expr ";" ]
          [ " v2 = " expr ";" ]
          [ " v3 = " expr ";" ]        
          [ " v4 = " expr ";" ]
          [ " v5 = " expr ";" ]
          "  y = " expr ";"

expr = expr op expr / 
       "(" expr op expr ")" /
       constant / "x[1]" / "x[2]" / "v1" / "v2" / "v3" / "v4" / "v5"      

op = "+" / "-" / "/" / "*"
 
constant = "1.0" / "0.5" / "0.2" / "0.1"

