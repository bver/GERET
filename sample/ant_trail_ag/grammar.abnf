; start symbol is <start>

start = code

code = main / func code

func = "def " identifier LF body "end" LF

main = "def step" LF body "end" LF

identifier = "fn1" / "fn2" / "fn3"

body = line LF / body line LF

line = if-statement / op / "fn1" / "fn2" / "fn3"

if-statement = "if ant.food_ahead" LF line LF "else" LF line LF "end"

op = "ant.left" / "ant.right" / "ant.move"

